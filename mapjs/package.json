{
  "name": "@mindmup/mapjs",
  "license": "MIT",
  "description": "Mind mapping visualisation library, using SVG, developed for MindMup",
  "type": "commonjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/mindmup/mapjs.git"
  },
  "keywords": [
    "mindmup",
    "svg"
  ],
  "files": [
    "src",
    "packages"
  ],
  "bugs": {
    "url": "https://github.com/mindmup/mapjs/issues"
  },
  "homepage": "https://www.mindmup.com",
  "version": "4.1.0",
  "scripts": {
    "env": "env",
    "report": "webpack --mode production --profile --json > stats.json; webpack-bundle-analyzer ./stats.json",
    "testc": "npm run test-start",
    "test-start": "npm run start && npm run test-cafe",
    "stop": "killall node",
    "start": "webpack-dev-server &",
    "test-cafe": "testcafe 'chrome:headless --no-default-browser-check' /home/s6mike/git_projects/argmap/test/devtools-recordings/add_supporting_group.json",
    "testcafe-command": "testcafe",
    "pack": "webpack",
    "lint": "eslint src",
    "pretest": "eslint specs src && rm -rf testem/compiled",
    "test-core": "node specs/support/jasmine-runner.js",
    "pretest-browser": "rm -rf testem/compiled",
    "test-browser": "testem ci -R dot",
    "debug-core": "node debug specs/support/jasmine-runner.js",
    "test": "npm run test-core && npm run test-browser",
    "test-watch": "npm run pretest && testem dev -R dot",
    "pretest-dev": "npm run pretest",
    "test-dev": "testem -l Chrome",
    "sourcemap": "sourcemap-lookup",
    "server": "webpack serve",
    "appraise": "appraise run --initial-width 500 --initial-height 300",
    "preappraise": "webpack --config webpack.appraise.config.js",
    "approve": "appraise approve"
  },
  "main": "src/npm-main.js",
  "dependencies": {
    "hammerjs": "^1.1.3",
    "jquery": "^3.6.1",
    "jquery-hammerjs": "^1.1.3",
    "jquery.hotkeys": "^0.1.0",
    "monotone-convex-hull-2d": "^1.0.1",
    "polybooljs": "^1.1.1",
    "underscore": "^1.13.6"
  },
  "devDependencies": {
    "eslint": "^5.4.0",
    "eslint-config-crockford": "^1.0.0",
    "eslint-config-defaults": "^9.0.0",
    "exports-loader": "^0.6.3",
    "fs-readdir-recursive": "^1.0.0",
    "imports-loader": "^0.7.1",
    "jasmine": "^2.7.0",
    "jasmine-spec-reporter": "^4.2.1",
    "sourcemap-lookup": "~0.0.5",
    "testcafe": "^1.20.1",
    "testem": "^3.9.0",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.7.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-dev-server": "^4.11.1"
  }
}
