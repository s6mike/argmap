    "sourcemap-lookup": "^0.0.5",
    "testcafe": "^2.3.1",
    "testem": "^3.10.1",
    "wait-on": "^7.0.1",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.7.0",
    "webpack-cli": "^5.0.1",
