  },
  "browser": "src/npm-main.js",
  "scripts": {
    "env": "env",
    "stop": "killall node",
    "stop:force": "fuser -k $PORT_DEV_SERVER/tcp",
    "start": "npm run start:dev",
    "start:dev": "webpack serve --mode=development &",
    "start:prod": "webpack serve --mode=production &",
    "testcafe": "testcafe 'chrome:headless --no-default-browser-check' /home/s6mike/git_projects/argmap/test/devtools-recordings/add_supporting_group.json",
    "testcafe:command": "testcafe",
    "pack": "webpack --mode=development &",
    "lint": "eslint src > lint_errors.txt",
    "report": "webpack --mode=production --profile --json > stats.json; webpack-bundle-analyzer ./stats.json"
  },
  "engines": {
    "node": "~16.13.1",
    "npm": "~9.2.0"
  },
  "overrides": {
    "find-babel-config@1.2.0": {
      "json5": "2.2.2"
    }
  },
  "dependencies": {
